{
  "manifest_version": 3,
  "name": "Custom Special Characters Shortcut",
  "version": "1.1",
  "description": "Type custom special characters using user-defined shortcuts.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  }
}